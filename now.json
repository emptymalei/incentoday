{
    "name": "incentoday",
    "version": 2,
    "env": {
        "MONGODB_URI": "@my-mongodb-uri"
    },
    "routes": [{
            "src": "/",
            "dest": "api/index.js",
            "methods": ["GET"]
        },
        {
            "src": "/kitchen",
            "dest": "api/kitchen.js",
            "methods": ["GET"]
        },
        {
            "src": "/kitchen",
            "dest": "api/kitchenUpdate.js",
            "methods": ["POST"]
        },
        {
            "src": "/users",
            "dest": "api/users.js",
            "methods": ["GET"]
        }
    ]
}